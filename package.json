{
  "name": "node-expose-sspi",
  "version": "0.0.15",
  "description": "Expose the Microsoft Windows SSPI interface in order to do NTLM and Kerberos authentication.",
  "main": "index.js",
  "scripts": {
    "test": "node test/index.js",
    "build": "node-gyp rebuild -j 8",
    "build-ts": "tsc -p src",
    "watch-ts": "tsc -p src -w",
    "dev": "nodemon -x \"npm run build && node test/index.js\"",
    "server": "nodemon test/server.js",
    "ts-index": "nodemon test/ts/index.ts",
    "ts-server": "nodemon test/ts/server.ts",
    "release": "standard-version",
    "example": "nodemon example/server.ts"
  },
  "keywords": [
    "SSPI",
    "SSO",
    "Single Sign On",
    "node-addon",
    "kerberos",
    "NTLM",
    "Negotiate",
    "SPNEGO",
    "authentication",
    "auth",
    "Microsoft",
    "Windows"
  ],
  "author": "Jean-Louis GUENEGO <jlguenego@gmail.com> (http://jlg-consulting.com/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/jlguenego/node-expose-sspi.git"
  },
  "license": "ISC",
  "gypfile": false,
  "dependencies": {
    "base64-arraybuffer": "^0.2.0",
    "http-errors": "^1.7.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.15.16",
    "@types/http-errors": "^1.6.3",
    "@types/serve-index": "^1.7.30",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "express-session": "^1.17.0",
    "node-addon-api": "^2.0.0",
    "nodemon": "^2.0.2",
    "serve-index": "^1.9.1",
    "standard-version": "^7.1.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  }
}
